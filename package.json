{
  "name": "js-projects",
  "version": "1.0.0",
  "scripts": {
    "test": "jest",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "test:e2e:basketball": "playwright test --project=basketball-reference-chromium",
    "test:e2e:basketball-all": "playwright test tests/e2e/Basketball-Reference",
    "test:e2e:mobile": "playwright test --project=basketball-reference-mobile",
